    <path refid="library.maven:_com.goldmansachs:gs-collections:5.1.0.classpath"/>
    <path refid="library.maven:_com.goldmansachs:gs-collections-api:5.1.0.classpath"/>
    <path refid="library.maven:_io.projectreactor:reactor-core:2.0.8.release.classpath"/>
    <path refid="library.maven:_org.reactivestreams:reactive-streams:1.0.0.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.qdl">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.qdl">
    <patternset refid="excluded.from.module.qdl"/>
  </patternset>
  
  <path id="qdl.module.sourcepath">
    <dirset dir="${module.qdl.basedir}">
      <include name="src/main/java"/>
      <include name="src/main/resources/dev_config"/>
      <include name="src/main/resources/prod_config"/>
      <include name="src/main/resources/test_config"/>
    </dirset>
  </path>
  
  <path id="qdl.module.test.sourcepath">
    <dirset dir="${module.qdl.basedir}">
      <include name="src/test/java"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.qdl" depends="compile.module.qdl.production,compile.module.qdl.tests" description="Compile module qdl"/>
  
  <target name="compile.module.qdl.production" depends="register.custom.compilers" description="Compile module qdl; production classes">
    <mkdir dir="${qdl.output.dir}"/>
    <javac2 destdir="${qdl.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.qdl}/javac">
      <compilerarg line="${compiler.args.qdl}"/>
      <bootclasspath refid="qdl.module.bootclasspath"/>
      <classpath refid="qdl.module.production.classpath"/>
      <src refid="qdl.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.qdl"/>
    </javac2>
    
    <copy todir="${qdl.output.dir}">
      <fileset dir="${module.qdl.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.qdl.basedir}/src/main/resources/dev_config">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.qdl.basedir}/src/main/resources/prod_config">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.qdl.basedir}/src/main/resources/test_config">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.qdl.tests" depends="register.custom.compilers,compile.module.qdl.production" description="compile module qdl; test classes" unless="skip.tests">
    <mkdir dir="${qdl.testoutput.dir}"/>
    <javac2 destdir="${qdl.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.qdl}/javac">
      <compilerarg line="${compiler.args.qdl}"/>
      <bootclasspath refid="qdl.module.bootclasspath"/>
      <classpath refid="qdl.module.classpath"/>
      <src refid="qdl.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.qdl"/>
    </javac2>
    
    <copy todir="${qdl.testoutput.dir}">
      <fileset dir="${module.qdl.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.qdl" description="cleanup module">
    <delete dir="${qdl.output.dir}"/>
    <delete dir="${qdl.testoutput.dir}"/>
  </target>
</project>