<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.peipao.qdl.sms.dao.SmsDao">	<resultMap type="com.peipao.qdl.sms.model.Sms" id="sms">		<id property="id" column="id" />		<result property="phone" column="phone" />		<result property="content" column="content" />		<result property="createTime" column="create_time" />		<result property="sendTime" column="send_time" />		<result property="status" column="status" />		<result property="sendCount" column="send_count" />		<result property="templateId" column="template_id" />	</resultMap>	<insert id="insertSms" >	<![CDATA[		insert into			running.t_sms(id,content,phone,send_time,create_time,status,send_count,template_id)		values		(#{id},#{content},#{phone},#{sendTime},#{createTime},#{status},#{sendCount},#{templateId})	]]>	</insert>	<update id="updateSms" >	<![CDATA[		update running.t_sms set content=#{content},phone=#{phone},send_time=#{sendTime},		create_time=#{createTime},status = #{status},send_count=#{sendCount},template_id=#{templateId}		where id=#{id}		]]>	</update>	<select id="getSms" parameterType="Integer" resultMap="sms">	<![CDATA[		select * from running.t_sms where status = #{status} order by create_time		]]>	</select></mapper>